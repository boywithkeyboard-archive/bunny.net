{
  "name": "bunny.net",
  "version": "1.0.0",
  "author": "Samuel Kopp (https://samuelkopp.de)",
  "license": "Apache-2.0",
  "description": "JavaScript SDK for ðŸ‡ bunny.net",
  "funding": "https://github.com/sponsors/boywithkeyboard",
  "repository": "github:boywithkeyboard/bunny.net",
  "keywords": [
    "bunny",
    "bunny.net",
    "sdk",
    "dns",
    "cdn",
    "edge storage"
  ],
  "main": "./build/index.cjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "files": [
    "./build"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./build/index.mjs",
        "types": "./build/index.d.ts"
      },
      "require": {
        "default": "./build/index.cjs",
        "types": "./build/index.d.ts"
      }
    }
  },
  "scripts": {
    "build:cjs": "esbuild src/index.ts --bundle --minify --format=cjs --target=es2022 --outfile=build/index.cjs",
    "build:esm": "esbuild src/index.ts --bundle --minify --format=esm --target=es2022 --outfile=build/index.mjs",
    "build": "rimraf build && tsc && npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "esbuild": "^0.20.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=18"
  }
}
